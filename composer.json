{
    "name": "polymorphine/headers",
    "description": "HTTP Response headers middleware",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "polymorphine/cs": "@dev",
        "phpunit/phpunit": "^7.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shudd3r",
            "email": "q3.shudder@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Polymorphine\\Headers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polymorphine\\Headers\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "run-tests": "phpunit --configuration phpunit.xml.dist",
        "style-check": "polymorphine-cs"
    }
}
